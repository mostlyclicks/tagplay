<% content_for :body do %>
  <ul id="projects">
    <% @projects.each do |project| %>
      <li>
        <%= link_to project.name, refinery.projects_project_path(project) %><br />
        <%= raw project.tag_list.map { |t| link_to t, refinery.projects_tag_path(t)}.join(', ') %>
      </li>
    <% end %>
  </ul>
<% end %>

<%= render '/refinery/content_page' %>
